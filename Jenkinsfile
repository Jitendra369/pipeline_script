pipeline {
    agent any
    stages {
        stage("build") {
            steps {
                bat 'echo "building application"'
            }
        }

        stage("test") {
            steps {
                bat 'echo "testing application"'
            }
        }

        stage("deploy") {
            steps {
                bat 'echo "deploying application"'
            }
        }
    }
}
